@model IEnumerable<WebCore.DBContextofSQLserver.Nhanvien>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tennv)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sdtnv)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Diachinv)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gioitinh)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ngaysinh)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MatkNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tennv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sdtnv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Diachinv)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gioitinh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ngaysinh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MatkNavigation.Matk)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Manv">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Manv">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Manv">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
